		"transactionProcessors": [
			"ipos.hashgraph.rest",
			"ipos.hashgraph.transactions"
		],
		"rest": {
			"port": -1,
			"derivedPort": 2000,
			"handlers": [
				"ipos.hashgraph.rest"
			]
		},
		"blockLogger": {
			"loggerClass": "com.txmq.exo.persistence.couchdb.CouchDBBlockLogger",
			"parameters": [
				{ "key": "databaseName", "value": "ipos-"},
